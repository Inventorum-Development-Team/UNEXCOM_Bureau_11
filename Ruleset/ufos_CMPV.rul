  - type: STR_KGB_HIDEOUT_UFO
    battlescapeTerrainData:
      name: KGB_HIDEOUT_UFO
      mapDataSets:
        - BLANKS
        - IDT_OFFICE
        - SOVIET_FLAG
        - URBAN
      mapBlocks:
        - name: KGB_HIDEOUT01
          width: 10
          length: 10
          items:
            STR_TRANSMITTER:
              - [ 2, 4, 0]
        - name: KGB_HIDEOUT02
          width: 10
          length: 10
          items:
            STR_TRANSMITTER:
              - [ 5, 4, 2]
        - name: KGB_HIDEOUT03
          width: 10
          length: 10
          items:
            STR_TRANSMITTER:
              - [ 3, 6, 2]

