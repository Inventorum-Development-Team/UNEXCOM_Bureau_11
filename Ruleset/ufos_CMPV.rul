        - BLANKS
        - U_EXT02
        - U_WALL02
        - U_BITS
        - CRASHED_UFO_TILES
      mapBlocks:
        - name: UFO_CRASH_1_02
          width: 10
          length: 20
  - type: STR_DISCOVERIES_LARGE_SCOUT
