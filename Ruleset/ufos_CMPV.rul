      name: DISCOVERIES_LARGE_SCOUT
      mapDataSets:
        - BLANKS
        - U_EXT02
        - U_WALL02
        - U_BITS
        - CRASHED_UFO_TILES
      mapBlocks:
        - name: UFO_CRASH_1_00
          width: 20
          length: 30
            

